{
  "name": "distance-from",
  "version": "1.1.2",
  "description": "Calculate distance between two coordinates",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run lint && npm run test",
    "test": "npm run build && jest",
    "start": "node index.js",
    "prepack": "npm run build",
    "clean": "rm -rf ./dist",
    "release": "npm publish --access public",
    "build": "npm run clean && tsc -p tsconfig.json",
    "build:w": "tsc -p tsconfig.json -w",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' && lint-staged"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-user/repo-url.git"
  },
  "keywords": [
    "haversine",
    "distance-from",
    "coordinates",
    "distance-between",
    "distance",
    "latitude",
    "longitude",
    "lat",
    "long"
  ],
  "author": "rickyplouis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rickyplouis/distance-from/issues"
  },
  "homepage": "https://github.com/rickyplouis/distance-from#readme",
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "prettier": "^1.16.1",
    "@types/node": "^12.0.4",
    "coveralls": "^3.0.3",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.0.1",
    "lint-staged": "^9.2.0"
  },
  "dependencies": {
    "amonad": "^0.8.2"
  },
  "contributors": [
    "airtucha"
  ]
}
